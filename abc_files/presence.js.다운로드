function createGuid() {
    return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) { var r = Math.random() * 16 | 0, v = c === 'x' ? r : (r & 0x3 | 0x8); return v.toString(16); });
}
function GetPresence10x10(userId, userName, userEmail) {
    var defaultHtml = HMG_Strings.Presence.DefaultHtml;
    var sId = "imn_" + createGuid() + ",type=sip";
    var presenceHtml = String.format(defaultHtml, userEmail, sId, userId, userName);
    return presenceHtml;
}
function GetPresenceOnlyPicture(userId, userName, userEmail, userPictureUrl, extensionNumber, curuserId, companyNumber, phoneNumber, companyCode, locationCode) {
    var defaultHtml = HMG_Strings.Presence.DefaultImageHtml40;
    var sId = "imn_" + createGuid() + ",type=sip";

    if (userPictureUrl == "") userPictureUrl = HMG_HostUrl + "/Shared/images/icon-anonymous.png";
    else userPictureUrl = userPictureUrl + ", " + HMG_HostUrl + "/Shared/images/icon-anonymous.png";

    extensionNumber = extensionNumber != undefined ? extensionNumber : "";
    companyNumber = companyNumber != undefined ? companyNumber : "";
    phoneNumber = phoneNumber != undefined ? phoneNumber : "";

    GetPresenceConfig(curuserId, companyCode);

    var mail = config.Mail == "Y" ? "on" : "off";
    var lync = config.Lync == "Y" ? "on" : "off";
    var mPlusNote = config.MPlusNote == "Y" ? "on" : "off";
    var sendFile = config.SendFile == "Y" ? "on" : "off";
    var phoneCall = config.PhoneCall == "Y" ? "on" : "off";
    var sms = config.SMS == "Y" ? "on" : "off";
    var like = config.Like == "Y" ? "on" : "off";

    defaultHtml = defaultHtml.replace("{Mail}", mail).replace("{MailValue}", config.Mail);
    defaultHtml = defaultHtml.replace("{Lync}", lync).replace("{LyncValue}", config.Lync);
    defaultHtml = defaultHtml.replace("{MPlusNote}", mPlusNote).replace("{MPlusNoteValue}", config.MPlusNote);
    defaultHtml = defaultHtml.replace("{SendFile}", sendFile).replace("{SendFileValue}", config.SendFile);
    defaultHtml = defaultHtml.replace("{PhoneCall}", phoneCall).replace("{PhoneCallValue}", config.PhoneCall);
    defaultHtml = defaultHtml.replace("{SMS}", sms).replace("{SMSValue}", config.SMS);
    defaultHtml = defaultHtml.replace("{Like}", like).replace("{LikeValue}", config.Like);

    var presenceHtml = String.format(defaultHtml, userPictureUrl, userId, userEmail, sId, extensionNumber,
        HMG_Strings.Presence.CallOuts.UserInfo, HMG_Strings.Presence.CallOuts.MailTo, HMG_Strings.Presence.CallOuts.Lync, HMG_Strings.Presence.CallOuts.MPlusNote, HMG_Strings.Presence.CallOuts.SendFile, HMG_Strings.Presence.CallOuts.PhoneCall, HMG_Strings.Presence.CallOuts.SMS, HMG_Strings.Presence.CallOuts.Like, userName, curuserId, companyNumber, phoneNumber, HMG_Strings.Presence.CallOuts.ExtensionNumber, HMG_Strings.Presence.CallOuts.CompanyPhone, HMG_Strings.Presence.CallOuts.CellPhone, locationCode, companyCode);

    return presenceHtml;
}
function GetPresenceOnlyPicture1(userId, userName, userEmail, userPictureUrl, extensionNumber, curuserId, companyNumber, phoneNumber, companyCode, locationCode) {
    var defaultHtml = HMG_Strings.Presence.DefaultImageHtml30;
    var sId = "imn_" + createGuid() + ",type=sip";

    if (userPictureUrl == "") userPictureUrl = "url(" + HMG_HostUrl + "/Shared/images/icon-anonymous.png)";
    else userPictureUrl = "url(" + userPictureUrl + "), url(/Shared/images/icon-anonymous.png)";

    extensionNumber = extensionNumber != undefined ? extensionNumber : "";
    companyNumber = companyNumber != undefined ? companyNumber : "";
    phoneNumber = phoneNumber != undefined ? phoneNumber : "";

    GetPresenceConfig(curuserId, companyCode);

    var mail = config.Mail == "Y" ? "on" : "off";
    var lync = config.Lync == "Y" ? "on" : "off";
    var mPlusNote = config.MPlusNote == "Y" ? "on" : "off";
    var sendFile = config.SendFile == "Y" ? "on" : "off";
    var phoneCall = config.PhoneCall == "Y" ? "on" : "off";
    var sms = config.SMS == "Y" ? "on" : "off";
    var like = config.Like == "Y" ? "on" : "off";

    defaultHtml = defaultHtml.replace("{Mail}", mail).replace("{MailValue}", config.Mail);
    defaultHtml = defaultHtml.replace("{Lync}", lync).replace("{LyncValue}", config.Lync);
    defaultHtml = defaultHtml.replace("{MPlusNote}", mPlusNote).replace("{MPlusNoteValue}", config.MPlusNote);
    defaultHtml = defaultHtml.replace("{SendFile}", sendFile).replace("{SendFileValue}", config.SendFile);
    defaultHtml = defaultHtml.replace("{PhoneCall}", phoneCall).replace("{PhoneCallValue}", config.PhoneCall);
    defaultHtml = defaultHtml.replace("{SMS}", sms).replace("{SMSValue}", config.SMS);
    defaultHtml = defaultHtml.replace("{Like}", like).replace("{LikeValue}", config.Like);

    var presenceHtml = String.format(defaultHtml, userPictureUrl, userId, userEmail, sId, extensionNumber,
        HMG_Strings.Presence.CallOuts.UserInfo, HMG_Strings.Presence.CallOuts.MailTo, HMG_Strings.Presence.CallOuts.Lync, HMG_Strings.Presence.CallOuts.MPlusNote, HMG_Strings.Presence.CallOuts.SendFile, HMG_Strings.Presence.CallOuts.PhoneCall, HMG_Strings.Presence.CallOuts.SMS, HMG_Strings.Presence.CallOuts.Like, userName, curuserId, companyNumber, phoneNumber, HMG_Strings.Presence.CallOuts.ExtensionNumber, HMG_Strings.Presence.CallOuts.CompanyPhone, HMG_Strings.Presence.CallOuts.CellPhone, locationCode, companyCode);

    return presenceHtml;
}
function GetPresenceOnlyPicture2(userId, userName, userEmail, userPictureUrl, extensionNumber, curuserId, companyNumber, phoneNumber, companyCode, locationCode) {
    var defaultHtml = HMG_Strings.Presence.DefaultImageHtml40;
    var sId = "imn_" + createGuid() + ",type=sip";

