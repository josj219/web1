    return presenceHtml;
}
function Imn_OpenPhoneList(obj, extensionNumber, companyNumber, phoneNumber, locationCode, companyCode, useYN) {
    if (companyCode != undefined && locationCode != undefined) {
        if ((companyCode == "H101" || companyCode == "K101") && locationCode != "1") {
            alert(HMG_Strings.Presence.Ready);
            return;
        }
    }

    if (useYN.toUpperCase() == "Y") {
        Imn_OpenPhoneCall(extensionNumber, useYN);
    }
    return;
    var $numbers = $(obj).parent().find(".c-user-profile__phone-numbers");

    if (extensionNumber == "" && companyNumber == "" && phoneNumber == "") {
        alert("No Phone number.");
    }
    else {
        if (extensionNumber == "") {
            $numbers.find(".c-user-profile__phone-number").eq(0).hide();
        }

        if (companyNumber == "") {
            $numbers.find(".c-user-profile__phone-number").eq(1).hide();
        }

        if (phoneNumber == "") {
            $numbers.find(".c-user-profile__phone-number").eq(2).hide();
        }

        //전화걸기 툴팁 토글
        if ($numbers.hasClass("active")) {
            $(obj).parent().find(".c-tooltip__box").show();
        }
        else {
            $(obj).parent().find(".c-tooltip__box").hide();
        }

        //전화걸기 목록 토글
        $numbers.toggleClass("active");
    }
}
function Imn_OpenUserProfile(id) {
    $("#NCUserID").val(id);
    var sFeatures = "scrollbars=0, toolbar=0";
    var d_width = 390;
    var d_height = 400;

    sFeatures += ',width=' + d_width;
    sFeatures += ',height=' + d_height;

    var dualScreeLeft = window.screenLeft;
    var dualScreeTop = window.screenTop;
    var width = window.innerWidth;
    var height = window.innerHeight;

    this.window.open("", "PopUpOpenUserProfileWindow", sFeatures);

    var OpenUserProfile = document.frmPopUpOpenUserProfile;
    OpenUserProfile.target = "PopUpOpenUserProfileWindow";
    OpenUserProfile.submit();
}

function Imn_OpenMail(id, name, useYN) {
    if (useYN.toUpperCase() != "Y") {
        return;
    }

    //$("#mode").val("WINDOW");
    //$("#cmd").val("PORTAL");
    var mailData = "<MailInfo><Receiver><To><Name>" + name + "</Name><Email>" + id + "</Email></To></Receiver><Subject></Subject><BodyType>HTML</BodyType><Body></Body><Files></Files></MailInfo>";
    $("#MailData").val(encodeURIComponent(mailData));

    url = mailDomain + "/WebMail/Mail/Write.aspx";

    var sFeatures = "scrollbars=yes, toolbar=no, resizable=yes";
    var d_width = window.screen.width;;
    var d_height = (window.screen.height - 82);

    sFeatures += ',width=' + d_width;
    sFeatures += ',height=' + d_height;

    var dualScreeLeft = window.screenLeft;
    var dualScreeTop = window.screenTop;
    var width = window.innerWidth;
    var height = window.innerHeight;

    var left = ((width / 2) - (d_width / 2)) + dualScreeLeft;
    var top = ((height / 2) - (d_height / 2)) + dualScreeTop;
    sFeatures += ',left=0,top=0';

    this.window.open(mailDomain + "/Blank.aspx", "PopUpOpenMailWindow", sFeatures);

    var OpenMailForm = document.frmPopUpOpenMail;

    if (OpenMailForm != null) OpenMailForm.focus();
    setTimeout(function () {
        OpenMailForm.target = "PopUpOpenMailWindow";
        OpenMailForm.submit();
    });
}
function Imn_OpenMessage(sip, useYN) {
    if (useYN.toUpperCase() != "Y") {
        return;
    }

    document.location.href = 'sip:' + sip;
}
function Imn_OpenMPlusNote(email, menuID, menuName, screenCode, screenName, id, useYN) {
    if (useYN.toUpperCase() != "Y") {
        return;
    }

    $("#receiver").val(id);
    $("#receiverMail").val(email);
    $("#MenuID").val(menuID);
    $("#MenuName").val(menuName);
    $("#ScreenCode").val(screenCode);
    $("#ScreenName").val(screenName);
    $("#IsPopup").val('true');

    var sFeatures = "scrollbars=0, toolbar=0";
    var d_width = 560;
    var d_height = 460;

    sFeatures += ',width=' + d_width;
    sFeatures += ',height=' + d_height;

    var dualScreeLeft = window.screenLeft;
    var dualScreeTop = window.screenTop;
    var width = window.innerWidth;
    var height = window.innerHeight;

    var left = ((width / 2) - (d_width / 2)) + dualScreeLeft;
    var top = ((height / 2) - (d_height / 2)) + dualScreeTop;
    sFeatures += ',left=' + left;
    sFeatures += ',top=' + top;

    var url = HMG_HostUrl + "/main/MPlus/Note.aspx";

    this.window.open("", "PopUpMPlusNoteWindow", sFeatures);

    var MPlusNoteForm = document.frmPopUpMPlusNote;
    MPlusNoteForm.target = "PopUpMPlusNoteWindow";
    MPlusNoteForm.submit();
}

function Imn_OpenPhoneCall(tel, useYN) {
    //document.location.href = 'callto:' + tel;
    try {
        if (useYN.toUpperCase() != "Y") {
            return;
        }

        var simpleLogin = new ActiveXObject("HMGPlugIn.SimpleLogin");
        if (simpleLogin == null) {
            // HMGPlugIn.SimpleLogin이 시스템에 설치되지 않은 경우의 처리.
            alert("HMGPlugIn.SimpleLogin was not installed.");
        }
        else {
            var rtn = simpleLogin.Exec("ucvoice\nM\nD\n" + tel);
            if (rtn == "2")
                alert("실행파일이 없습니다.");
        }
    }
    catch (e) {
        alert("HMGPlugIn.SimpleLogin was not installed.");
    }
}
function Imn_OpenSMS(id, email, menuID, menuName, screenCode, screenName, useYN) {
    if (useYN.toUpperCase() != "Y") {
        return;
    }

    $("#SMSUserID").val(id);
    $("#SMSEmail").val(email);
    $("#SMSMenuID").val(menuID);
    $("#SMSMenuName").val(menuName);
    $("#SMSScreenCode").val(screenCode);
    $("#SMSScreenName").val(screenName);
    $("#SMSIsPopup").val('true');

    var sFeatures = "scrollbars=0, toolbar=0";
    var d_width = 460;
    var d_height = 450;

    sFeatures += ',width=' + d_width;
    sFeatures += ',height=' + d_height;

    var dualScreeLeft = window.screenLeft;
    var dualScreeTop = window.screenTop;
    var width = window.innerWidth;
    var height = window.innerHeight;

    var left = ((width / 2) - (d_width / 2)) + dualScreeLeft;
    var top = ((height / 2) - (d_height / 2)) + dualScreeTop;
    sFeatures += ',left=' + left;
    sFeatures += ',top=' + top;

    var url = HMG_HostUrl + "/main/MPlus/SMS.aspx";

    this.window.open("", "PopUpSMSWindow", sFeatures);

    var SMSForm = document.frmPopUpSMS;
    SMSForm.target = "PopUpSMSWindow";
    SMSForm.submit();
}
function Imn_SendFile(sender, receiver, useYN) {
    if (useYN.toUpperCase() != "Y") {
        return;
    }

    try {
        var simpleLogin = new ActiveXObject("HMGPlugIn.SimpleLogin");
        if (simpleLogin == null) {
            // HMGPlugIn.SimpleLogin이 시스템에 설치되지 않은 경우의 처리.
            alert("HMGPlugIn.SimpleLogin was not installed.");
        }
        else {
            if (sender == "undefined" || $.isEmptyObject(sender)) {
                alert("발신자 메일 정보가 없습니다.");
                return;
            }
            else if (receiver == "undefined" || $.isEmptyObject(receiver)) {
                alert("수신자 메일 정보가 없습니다.");
                return;
            }

            var rtn = simpleLogin.Exec("axonmsg\nfile\n/sip:" + sender + "\n/Contacts=<sip:" + receiver + ">");
            if (rtn == "2")
                alert("실행파일이 없습니다.");
        }
    }
    catch (e) {
        alert("HMGPlugIn.SimpleLogin was not installed.");
    }
}
function Imn_Like(id, useYN) {
    if (useYN.toUpperCase() != "Y") {
        return;
    }

    $("#LikeUserID").val(id);
    var sFeatures = "scrollbars=0, toolbar=0";
    var d_width = 420;
    var d_height = 180;

    sFeatures += ',width=' + d_width;
    sFeatures += ',height=' + d_height;

    var dualScreeLeft = window.screenLeft;
    var dualScreeTop = window.screenTop;
    var width = window.innerWidth;
    var height = window.innerHeight;

    this.window.open("", "PopUpOpenLikeUserWindow", sFeatures);

    var OpenLikeUserForm = document.frmPopUpOpenLikeUser;
    OpenLikeUserForm.target = "PopUpOpenLikeUserWindow";
    OpenLikeUserForm.submit();
}
function ClearProfileCard() {
    $('.c-user-profile--open').toggleClass('c-user-profile--open');
}

// presence Config 데이터 가져오기 (2018-11-28 추가)
function GetPresenceConfig(userId, companyCode) {

    if (config == null || config == undefined) {
        $.ajax({
            type: "POST",
            url: HMG_HostUrl + "/_vti_bin/exapi/Config/GetPresenceConfig",
            cache: false,
            data: 'CompanyCode=' + companyCode + '&UserID=' + userId,
            async: false,
            datatype: "json",
            success: function (data) {
                config = data;
            },
            error: function (response) {
                config = {
                    Mail: "Y"
                    , Lync: "Y"
                    , MPlusNote: "Y"
                    , SendFile: "Y"
                    , PhoneCall: "Y"
                    , SMS: "Y"
                    , Like: "Y"
                };
                //config = [];
                //config.push({
                //    Mail: "Y"
                //    , Lync: "Y"
                //    , MPlusNote: "Y"
                //    , SendFile: "Y"
                //    , PhoneCall: "Y"
                //    , SMS: "Y"
                //    , Like: "Y"
                //});
                //config.Mail = "Y";
                //config.Lync = "Y";
                //config.MPlusNote = "Y";
                //config.SendFile = "Y";
                //config.PhoneCall = "Y";
                //config.SMS = "Y";
                //config.Like = "Y";
            }
        });
    }
}


$(document).ready(function () {
    $('.c-user-profile__visual').on('click', function (e) {
        if ($(this).parent().hasClass('c-user-profile--open'))
            ClearProfileCard();
        else {
            ClearProfileCard();
            $(this).parent().toggleClass('c-user-profile--open');
        }
        //e.preventDefault();
    });

    // R&D 구분
    if (HMG_HostUrl.toLowerCase().indexOf("rnd") > -1 || HMG_HostUrl.toLowerCase().indexOf("rndwf") > -1 || HMG_HostUrl.toLowerCase().indexOf("rndmail") > -1) {
        HMG_HostUrl = location.protocol + "//rndportal" + "." + HMG_Domain;
    }
    else if (HMG_Domain == "") {
        HMG_HostUrl = HMG_HostUrl;
    }
    else {
        HMG_HostUrl = location.protocol + "//autoway" + "." + HMG_Domain;
    }

    $('body').append("<div style='display:none;'><form name='frmPopUpOpenUserProfile' method='post' target='PopUpOpenUserProfileWindow' action='" + HMG_HostUrl + "/Main/UserInfoPop/NameCard.aspx'><input name='NCUserID' id='NCUserID' type='hidden' value=''/></form>" +
        "<form name='frmPopUpMPlusNote' method='post' target='PopUpMPlusNoteWindow' action='" + HMG_HostUrl + "/Main/MPlus/Note.aspx'><input name='receiver' id='receiver' type='hidden' value=''/><input name='receiverMail' id='receiverMail' type='hidden' value=''/><input name='MenuID' id='MenuID' type='hidden' value=''/><input name='MenuName' id='MenuName' type='hidden' value=''/><input name='ScreenCode' id='ScreenCode' type='hidden' value=''/><input name='ScreenName' id='ScreenName' type='hidden' value=''/><input name='IsPopup' id='IsPopup' type='hidden' value=''/></form>" +
        "<form name='frmPopUpOpenMail' method='post' target='PopUpOpenMailWindow' action='" + mailDomain + "/WebMail/Mail/Write.aspx'><input name='mode' id='mode' type='hidden' value='WINDOW'/><input name='cmd' id='cmd' type='hidden' value='PORTAL'/><input name='MailData' id='MailData' type='hidden' value=''/></form>" +
        "<form name='frmPopUpSMS' method='post' target='PopUpSMSWindow' action='" + HMG_HostUrl + "/main/MPlus/SMS.aspx'><input name='SMSUserID' id='SMSUserID' type='hidden' value=''/><input name='SMSEmail' id='SMSEmail' type='hidden' value=''/><input name='SMSMenuID' id='SMSMenuID' type='hidden' value=''/><input name='SMSMenuName' id='SMSMenuName' type='hidden' value=''/><input name='SMSScreenCode' id='SMSScreenCode' type='hidden' value=''/><input name='SMSScreenName' id='SMSScreenName' type='hidden' value=''/><input name='SMSIsPopup' id='SMSIsPopup' type='hidden' value=''/></form>" +
        "<form name='frmPopUpOpenLikeUser' method='post' target='PopUpOpenLikeUserWindow' action='" + HMG_HostUrl + "/Main/UserInfoPop/LikeUser.aspx'><input name='LikeUserID' id='LikeUserID' type='hidden' value=''/></form></div>");
});