String.format = function () {
    var formatString = arguments[0]; for (var i = 1; i < arguments.length; i++) { var regEx = new RegExp("\\{" + (i - 1) + "\\}", "gm"); formatString = formatString.replace(regEx, arguments[i]); } return formatString;
